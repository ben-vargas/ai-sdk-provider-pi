{
  "name": "ai-sdk-provider-pi",
  "version": "0.1.0",
  "description": "Vercel AI SDK v6 provider for Pi coding agent",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "ai-sdk",
    "provider",
    "pi",
    "vercel"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/provider": "3.0.8",
    "@ai-sdk/provider-utils": "4.0.15",
    "@mariozechner/pi-agent-core": "0.52.12",
    "@mariozechner/pi-ai": "0.52.12",
    "@mariozechner/pi-coding-agent": "0.52.12",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@types/node": "25.2.3",
    "@vitest/coverage-v8": "4.0.18",
    "ai": "6.0.86",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  }
}
