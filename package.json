{
  "name": "ai-sdk-provider-pi",
  "version": "0.1.0",
  "description": "Vercel AI SDK v6 provider for Pi coding agent",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "clean": "rm -rf dist coverage .vitest",
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "ci": "npm run format:check && npm run lint && npm run typecheck && npm run test",
    "prepare": "npm run build",
    "prepublishOnly": "npm run clean && npm run ci && npm run build",
    "example:basic": "npx tsx examples/basic-usage.ts",
    "example:streaming": "npx tsx examples/streaming.ts",
    "example:events": "npx tsx examples/streaming-events.ts",
    "example:tools": "npx tsx examples/tool-calling.ts",
    "example:session": "npx tsx examples/session-management.ts",
    "example:memory": "npx tsx examples/memory-comparison.ts",
    "example:object": "npx tsx examples/generate-object-basic.ts",
    "example:image": "npx tsx examples/image-analysis.ts",
    "example:abort": "npx tsx examples/abort-signal.ts",
    "example:auth": "npx tsx examples/error-handling-auth.ts",
    "example:logging": "npx tsx examples/logging-custom-logger.ts"
  },
  "keywords": [
    "ai-sdk",
    "provider",
    "pi",
    "vercel"
  ],
  "author": "Ben Vargas",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/provider": "3.0.8",
    "@ai-sdk/provider-utils": "4.0.15",
    "@mariozechner/pi-agent-core": "0.52.12",
    "@mariozechner/pi-ai": "0.52.12",
    "@mariozechner/pi-coding-agent": "0.52.12",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "25.2.3",
    "@vitest/coverage-v8": "4.0.18",
    "ai": "6.0.86",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "tsup": "8.5.1",
    "typescript-eslint": "^8.46.4",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  }
}
